        sh """
           echo "First Stage"
        """
      }
    }
    stage('second-stage') {
      steps {
        sh """
          echo "Second Stage"
        """
      }
    }
    stage('third-stage') {
      steps {
        sh """
          echo "Third Stage"
        """
      }
    }
     stage("fourth-stage") {
      steps {
        sh """
          echo "Fourth Stage"
        """
      }
    }
  }
}
